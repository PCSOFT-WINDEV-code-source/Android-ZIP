      procedure FEN_SMARTPHONE_SELECTEURREP(gsTitreSélecteur est une chaine = "", gsDossierSélectionné est une chaine = "")
      
      	DOSSIER_ENCOURS est une chaine
      	
      	MaFenêtre..ValeurRenvoyée = ""
      
      	// Titre du sélecteur
      	si gsTitreSélecteur <> "" ALORS
      		mafenetre..titre = gsTitreSélecteur
      	FIN
