  procedures :
   -
     name : ListeEléments
     procedure_id : 2240998056841498722
     type_code : 14
     code : |1-
      // Résumé : Liste les dossiers et les archives d'un dossier
      // Syntaxe :
      //ListeEléments ( [<sRacine> est chaîne])
      //
      // Paramètres :
      //	sRacine (chaîne UNICODE - valeur par défaut="") : Racine du disque à partir de laquelle on doit lister les éléments
      // Valeur de retour :
      // 	Aucune
      //
      //	sArchive (chaîne UNICODE - valeur par défaut="") : Nom de l'archive en cours
      PROCEDURE ListeEléments(local sRacine est une chaine = "")
      
      	sListeRépertoires	est une chaine
      	sRépertoire			est une chaine
      	sParent				est une chaine
      	
      	// Complète
      	Trace("Récupération de la liste des dossiers de : " + (sRacine = "" ? "<Racine>" sinon sRacine))
      	
      	LIB_CHEMIN = (sRacine = "" ? "Aucun dossier en cours" SINON [SEPARATEUR] + Remplace(Remplace(sRacine, ARCHIVES, "Archives"), SysRepCarteStockage(), "SDCARD"))
      	DOSSIER_ENCOURS = sRacine
      
      	// Vide la liste
      	ZoneRépétéeSupprimeTout(ZR_FICHIERS)
      
      	// Si on est à la racine
      	si sRacine = "" ALORS
      		
      		Trace("Activation des dossiers racines...")
      		
      		// Dossier de l'application
      		ZoneRépétéeAjouteLigne(ZR_FICHIERS, IMG_EXEMPLE, "Archives", ARCHIVES, frrepertoire, "")
      		
      		si pas EnModeSimulateur alors
      			// Si une SDCARD est active
      			si SysEtatCarteStockage() <> sysCarteIndisponible ALORS
      				// On ajoute la racine du système
      				ZoneRépétéeAjouteLigne(ZR_FICHIERS, IMG_SDCARD, "SDCARD", SysRepCarteStockage(), frRépertoire, "")
      			FIN			
      		fin
      		
      		OPT_OK..etat = grise
      	sinon
      		
      		// Chemin parent
      		si sRacine = SysRepCarteStockage() _ou_ sRacine = ARCHIVES ALORS
      			sParent = ""
      		sinon
      			sParent = sRacine[[a PositionOccurrence(sRacine, SEPARATEUR, rangDernier) - 1]]
      		FIN
      		Trace("Dossier parent : " + (sParent = "" ? "<Racine>" SINON sParent))
      		
      		// Liste les répertoires
      		Trace("Recherche de dossier depuis : " + sRacine)
      		sListeRépertoires = fListeRépertoire(sRacine, frNonRécursif)
      		Trace(ChaîneOccurrence(sListeRépertoires, rc) + " dossier(s) listé(s)")
      		
      	
      		// Pour chaque répertoire
      		pour toute chaine sRépertoire de sListeRépertoires separee par rc		
      			ZoneRépétéeAjouteLigne(ZR_FICHIERS, IMG_DOSSIER, extraitchaine(sRépertoire, rangdernier, SEPARATEUR), sRépertoire, frRépertoire, "")
      		FIN
      			
      		// Ajoute un élément pour remonter l'arborescence
      		ZoneRépétéeinsèreLigne(ZR_FICHIERS, 1, IMG_DOSSIER_PARENT, "", sParent, frRépertoire, "")
      		
      		OPT_OK..Etat = actif
      	FIN
