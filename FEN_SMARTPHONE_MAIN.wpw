        code : |1-
         
         sElément est une chaine = ATT_CHEMINCOMPLET
         
         Trace("Elément sélectionné : [" + ATT_TYPE + "]; " + sElément)
         
         // Si c'est un dossier
         si ATT_TYPE = frRépertoire alors
         	
         	// Ferme l'archive précédente si nécessaire
         	SI ARCHIVE_OUVERTE ALORS
         		zipFerme("zipArchive")
         		supprimetout(ARCHIVE_FICHIERS)
         		ARCHIVE_ENCOURS 	= ""
         		ARCHIVE_OUVERTE 	= Faux
         	FIN
         	
         	// Liste les éléments du dossier sélectionné
         	ListeEléments(sElément)
         
         
         // Si c'est une archive ZIP
         sinon si ATT_TYPE = frfichier alors
         	
         	// Ferme l'archive précédente si nécessaire
         	si ARCHIVE_OUVERTE ALORS
         		zipferme("zipArchive")
         		SupprimeTout(ARCHIVE_FICHIERS)
         		ARCHIVE_ENCOURS 	= ""
         		ARCHIVE_OUVERTE 	= faux
         	FIN
         	
         	// On charge l'archive
         	zipOuvre("zipArchive", sElément)
         	ARCHIVE_ENCOURS = sElément
         	ARCHIVE_OUVERTE = vrai
         	
         	// Liste les éléments de l'archive
         	ListeEléments("", "zipArchive")	
         	
         // Si c'est un dossier dans l'archive
         sinon si ATT_TYPE = frZipRépertoire alors
         	
         	// Liste les éléments de l'archive
         	ListeEléments(sElément, "zipArchive")	
         		
         // Si c'est un fichier dans l'archive
         sinon si ATT_TYPE = frZipFichier alors
         	
         	// On ne fait rien
         fin
