   -
     name : ExtraireDans
     procedure_id : 2240998563647693033
     type_code : 14
     code : |1-
      // Résumé : Extrait l'archive actuellement ouverte dans le répertoire spécifié
      // Syntaxe :
      //ExtraireDans (<sRepExtraction> est chaîne)
      //
      // Paramètres :
      //	sRepExtraction (chaîne UNICODE) : Répertoire dans lequel sera extraite l'archive
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ExtraireDans(local sRepExtraction est une chaine)
      
      	nIndice 			est un entier
      	nErr				est un entier
      	sRepExtractionBkp	est une chaine = sRepExtraction
      	
      	REPEXTRACTION = ""
      	OuvreFenêtreMobile(FEN_JAUGE_EXTRACTION)
      	
      	// Extraction complète de l'archive
      	si ATT_TYPE = frfichier ALORS
      		
      		Trace("Extraction de " + ATT_CHEMINCOMPLET + " dans : " + sRepExtractionBkp)
      		nErr = zipouvre("zipArchiveTemp", ATT_CHEMINCOMPLET, zipLecture)
      		si nErr = 0 ALORS
      		
      			sablier(vrai)
      			
      			FEN_JAUGE_EXTRACTION.JAUGE_EXTRACTION = 0
      			FEN_JAUGE_EXTRACTION.JAUGE_EXTRACTION..BorneMax = zipNbFichier("zipArchiveTemp")
      			FEN_JAUGE_EXTRACTION.JAUGE_EXTRACTION..Visible = vrai
      			
      			pour i = 1 _a_ FEN_JAUGE_EXTRACTION.JAUGE_EXTRACTION..BorneMax
      				
      				// Informe sur le fichier en cours d'extraction
      				FEN_JAUGE_EXTRACTION.LIB_INFO_EXTRACTION = "Extraction du fichier" + RC + ExtraitChaîne(zipInfoFichier("zipArchiveTemp", i), 1) + "..."
      				
      				// Extrait le fichier
      				Trace("Extraction de " + ExtraitChaîne(zipInfoFichier("zipArchiveTemp", i), 1) + " dans : " + sRepExtractionBkp)
      				nErr = zipExtraitFichier("zipArchiveTemp", i, sRepExtractionBkp)
      				SI nErr <> 0 ALORS
      					Erreur("Impossible d'extraire le fichier", zipMsgErreur(nErr))
      					SORTIR
      				FIN
      				FEN_JAUGE_EXTRACTION.JAUGE_EXTRACTION ++
      			FIN
      			
      			FEN_JAUGE_EXTRACTION.LIB_INFO_EXTRACTION = ""
      			zipferme("zipArchiveTemp")
      			
      			sablier(faux)
      		sinon
      			Erreur("Impossible d'extraire l'archive", zipMsgErreur(nErr))
      		FIN		
      		
      	// Extraction d'un fichier de l'archive
      	sinon si ATT_TYPE = frzipfichier alors
      		
      		sablier(vrai)
      		
      		// Informe sur le fichier en cours d'extraction
      		Trace("Extraction de " + ATT_CHEMINCOMPLET + " dans : " + sRepExtractionBkp)
      		FEN_JAUGE_EXTRACTION.LIB_INFO_EXTRACTION = "Extraction du fichier" + RC + ATT_CHEMINCOMPLET + "..."
      		
      		nErr = zipExtraitFichier(ARCHIVE_ENCOURS, ATT_CHEMINCOMPLET, sRepExtractionBkp)
      		si nErr <> 0 alors erreur("Impossible d'extraire le fichier", zipMsgErreur(nErr))
      		
      		FEN_JAUGE_EXTRACTION.LIB_INFO_EXTRACTION = ""
      		Sablier(faux)
      		
      	// Extraction d'un répertoire complet de l'archive
      	sinon si ATT_TYPE = frZipRépertoire alors
      		
      		Sablier(Vrai)
      				
      		Trace("Extraction de " + ATT_CHEMINCOMPLET + " dans : " + sRepExtractionBkp)
      				
      		// Premier fichier du répertoire
      		nIndice = zipChercheFichier(archive_encours, ATT_CHEMINCOMPLET, Faux, 1)
      		tantque nIndice > 0
      			
      			// Informe sur le fichier en cours d'extraction
      			FEN_JAUGE_EXTRACTION.LIB_INFO_EXTRACTION = "Extraction du fichier" + rc + extraitchaine(zipInfoFichier(ARCHIVE_ENCOURS, nIndice), 1) + "..."
      		
      			// Extrait le fichier
      			Trace("Extraction de " + ExtraitChaîne(zipInfoFichier(ARCHIVE_ENCOURS, nIndice), 1) + " dans : " + sRepExtractionBkp + ATT_LIBELLE)
      			nErr = zipExtraitFichier(ARCHIVE_ENCOURS, nIndice, sRepExtractionBkp + [separateur] + ATT_LIBELLE)
      			si nErr <> 0 ALORS
      				Erreur("Impossible d'extraire le fichier " + ExtraitChaîne(zipInfoFichier(ARCHIVE_ENCOURS, nIndice), 1), zipMsgErreur(nErr))
      				sortir
      			FIN
      			
      			// Fichier suivant
      			nIndice = zipChercheFichier(ARCHIVE_ENCOURS, ATT_CHEMINCOMPLET, Faux, nIndice + 1)
      		FIN
      		
      		FEN_JAUGE_EXTRACTION.LIB_INFO_EXTRACTION = ""
      		Sablier(faux)
      		
      	FIN
      	
      	Trace("Fin de l'extraction")
      	ferme(FEN_JAUGE_EXTRACTION)
