  procedures :
   -
     name : ListeEléments
     internal_properties : CAAAAAgAAADeb/F7rxIE/Wa25xAPBWumqOX69dq3WbA1P3q0+PXgz54HLREXYxmoqerDgfgsnrm0H2FAqqy3ygVlSoQRiEdKDzUNEoLwpVTdbqwPVWitwmkWtZxfcgXW9AYTRBVgjURpdsR3PIkMJ5CS+uEF
     procedure_id : 2240917350105618750
     type_code : 14
     code : |1-
      // Résumé : Liste les dossiers et les archives d'un dossier
      // Syntaxe :
      //ListeEléments ( [<sRacine> est chaîne [, <sArchive> est chaîne]])
      //
      // Paramètres :
      //	sRacine (chaîne UNICODE - valeur par défaut="") : Racine du disque à partir de laquelle on doit lister les éléments
      // 	sArchive (chaîne UNICODE - valeur par défaut="") : Nom de l'archive en cours
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ListeEléments(local sRacine est une chaine = "", local sArchive est une chaine = "")
      
      	sListeRépertoires	est une chaine
      	sListeFichiers		est une chaine
      	sRépertoire			est une chaine
      	sFichier			est une chaine
      	sParent				est une chaine
      	sInfosZIP			est une chaine
      	nNbFichiers			est un entier
      	sListeEléments		est une chaine
      	tabEléments			est un tableau de chaînes
      	tabDossiers			est un tableau de chaînes
      	sElément			est une chaine
      	sDossier			est une chaine
      	nProfondeur			est un entier
      	sInfoFichierComp	est une chaine
      	nProfondeurElément	est un entier
      	
      	// Complète
      	Trace("Récupération de la liste des fichiers et dossiers de : " + (["("] + sArchive + [") "]) + (sRacine = "" ? "<Racine>" sinon sRacine))
      
      	si sArchive = "" ALORS
      		REP_ENCOURS = sRacine
      		LIB_CHEMIN = (sRacine = "" ? "Aucun dossier en cours" sinon [SEPARATEUR] + remplace(remplace(sRacine, ARCHIVES, "Archives"), SysRepCarteStockage(), "SDCARD"))
      	sinon
      		LIB_CHEMIN = [SEPARATEUR] + Remplace(Remplace(ARCHIVE_ENCOURS, ARCHIVES, "Archives"), SysRepCarteStockage(), "SDCARD") + [SEPARATEUR] + sRacine
      	FIN
      
      	// Vide la liste
      	ZoneRépétéeSupprimeTout(ZR_FICHIERS)
      	
      	// Si pas d'archive en cours
      	si sArchive = "" alors
      	
      		// Si on est à la racine
      		si sRacine = "" ALORS
      			
      			Trace("Activation des dossiers racines...")
      			
      			// Dossier de l'application
      			ZoneRépétéeAjouteLigne(ZR_FICHIERS, IMG_EXEMPLE, "Archives", ARCHIVES, frrepertoire, "")
      			
      			si pas EnModeSimulateur alors
      				// Si une SDCARD est active
      				si SysEtatCarteStockage() <> sysCarteIndisponible ALORS
      					// On ajoute la racine du système
      					ZoneRépétéeAjouteLigne(ZR_FICHIERS, IMG_SDCARD, "SDCARD", SysRepCarteStockage(), frRépertoire, "")
      				sinon
      					// On ajoute la racine du système
      					ZoneRépétéeAjouteLigne(ZR_FICHIERS, IMG_SYSTEM, "Système", "/", frRépertoire, "")
      				FIN			
      			fin
      		sinon
      			
      			// Chemin parent
      			si sRacine = SysRepCarteStockage() _ou_ sRacine = "/" _ou_ sRacine = ARCHIVES ALORS
      				sParent = ""
      			sinon
      				sParent = sRacine[[a PositionOccurrence(sRacine, SEPARATEUR, rangDernier) - 1]]
      			FIN
      			Trace("Dossier parent : " + (sParent = "" ? "<Racine>" SINON sParent))
      			
      			// Liste les répertoires
      			Trace("Recherche de dossier depuis : " + sRacine)
      			sListeRépertoires = fListeRépertoire(sRacine, frNonRécursif)
      			Trace(ChaîneOccurrence(sListeRépertoires, rc) + " dossier(s) listé(s)")
      			
      			// Liste les fichiers
      			Trace("Recherche d'archives ZIP depuis : " + sRacine)
      			sListeFichiers	  = fListeFichier(sRacine + [SEPARATEUR] + "*.zip", frNonRécursif)
      			Trace(ChaîneOccurrence(sListeFichiers, RC) + " archive(s) ZIP listée(s)")
      			
      			// Si le dossier est vide
      			si sListeFichiers = "" _et_ sListeRépertoires = "" ALORS
      				
      				// Affiche un toast
      				ToastAffiche("Le dossier ne contient pas d'archive", toastcourt, cvMilieu, chcentre)			
      				
      				// On revient sur le parent
      				ListeEléments(sParent)
      			sinon
      			
      				// Pour chaque répertoire
      				pour toute chaine sRépertoire de sListeRépertoires separee par rc		
      					ZoneRépétéeAjouteLigne(ZR_FICHIERS, IMG_DOSSIER, extraitchaine(sRépertoire, rangdernier, SEPARATEUR), sRépertoire, frRépertoire, "")
      				FIN
      				
      				// Pour chaque fichier
      				pour toute chaine sFichier de sListeFichiers separee par rc
      					
      					// Ouvre l'archive
      					si zipOuvre("zipArchiveTemp", sFichier, zipLecture) = 0 ALORS
      						
      						// Récupère le nombre de fichiers présents dans l'archive
      						nNbFichiers = zipNbFichier("zipArchiveTemp")
      						
      						// Affiche la taille et le nombre de fichiers de l'archive
      						sInfosZIP = TailleVersChaîne(zipTaille("zipArchiveTemp", vrai)) + rc + 
      								    NumériqueVersChaîne(nNbFichiers, "dS") + " fichier" + (nNbFichiers > 1 ? "s" sinon "")
      						
      						// Ferme l'archive
      						zipferme("zipArchiveTemp")
      					sinon
      						sInfosZIP = "Archive invalide"
      					FIN
      					
      					ZoneRépétéeAjouteLigne(ZR_FICHIERS, IMG_ARCHIVE, fExtraitChemin(sFichier, fFichier + fExtension), sFichier, frfichier, sInfosZIP)
      				FIN
      		
      				// Ajoute un élément pour remonter l'arborescence
      				ZoneRépétéeinsèreLigne(ZR_FICHIERS, 1, IMG_DOSSIER_PARENT, "", sParent, frRépertoire, "")
      			fin
      		FIN
      	sinon
      		
      		si sRacine = "" ALORS
      									
      			// Ajoute un élément pour remonter l'arborescence
      			ZonerepeteeAjouteligne(ZR_FICHIERS, IMG_DOSSIER_PARENT, "", DécomplèteRep(fExtraitChemin(ARCHIVE_ENCOURS, fDisque + fRépertoire)), frRépertoire, "")				
      
      			// Stocke les fichiers de l'archive
      			si ARCHIVE_FICHIERS..Occurrence = 0 alors
      				
      				// Liste les fichiers contenus dans l'archive
      				sListeEléments = zipListeFichier("zipArchive")
      				// Transforme la liste en tableau
      				chaineverstableau(sListeEléments, tabEléments, rc)
      				// Trie le tableau
      				trie(tabEléments, ttCroissant)
      				
      				pour tout sElément de tabEléments
      					
      					// Extrait le dossier de l'élément
      					sDossier = DécomplèteRep(fExtraitChemin(sElément, fDisque + fRépertoire))
      					
      					// Si le dossier est vide, on est à la racine de l'archive, on ne prend donc pas en compte ce dossier
      					si sDossier <> "" alors					
      						// Si le dossier n'a pas encore été mémorisé
      						si cherche(tabDossiers, tcLinéaire, sDossier) < 1 ALORS
      							ajoute(tabDossiers, ChaîneOccurrence(sDossier, SEPARATEUR) + ";" + frZipRépertoire + ";" + sDossier)
      						FIN										
      						
      						// Ajoute le fichier à partir du dossier
      						ajoute(ARCHIVE_FICHIERS, ChaîneOccurrence(sDossier + [SEPARATEUR], SEPARATEUR) + ";" + frZipFichier + ";" + sElément)
      					sinon
      						// Ajoute le fichier de la racine
      						Ajoute(ARCHIVE_FICHIERS, "0;" + frZipFichier + ";" + sElément)
      					fin
      					
      				FIN
      				
      				// Insère les dossiers en début de liste
      				Insère(ARCHIVE_FICHIERS, 1, tabDossiers)
      				Trie(ARCHIVE_FICHIERS, ttCroissant)
      			fin
      			
      			// Pas de profondeur
      			nProfondeur = 0
      		sinon
      			// Niveau de profondeur demandé
      			nProfondeur	= ChaîneOccurrence(sRacine, SEPARATEUR) + 1
      			
      			// Ajoute un élément pour remonter l'arborescence
      			sParent = sRacine[[A PositionOccurrence(sRacine, SEPARATEUR, rangDernier) - 1]]
      			ZoneRépétéeajouteLigne(ZR_FICHIERS, IMG_DOSSIER_PARENT, "", sParent, frzipRépertoire, "")				
      		FIN
      		
      		// Liste les dossiers à la racine
      		pour tout sElément de ARCHIVE_FICHIERS
      			
      			nProfondeurElément = Val(ExtraitChaîne(sElément, 1, ";"))
      			
      			// Si l'élément listé ne fait pas partie du niveau de profondeur attendu
      			si nProfondeurElément > nProfondeur ALORS
      				sortir
      			sinon si nProfondeurElément < nProfondeur alors
      				continuer
      			FIN
      			
      			// Selon le type de l'élément
      			selon val(extraitchaine(sElément, 2, ";"))
      				
      				// C'est un répertoire
      				cas frZipRépertoire
      					
      					sRépertoire = extraitchaine(sElément, rangdernier, ";")
      					
      					// Vérifie que le répertoire se trouve dans la racine en cours
      					SI pas sRépertoire[[A PositionOccurrence(sRépertoire, SEPARATEUR, rangDernier) - 1]] = sRacine ALORS
      						continuer
      					FIN
      					
      					ZoneRépétéeAjouteLigne(ZR_FICHIERS, IMG_DOSSIER, ExtraitChaîne(sRépertoire, rangDernier, SEPARATEUR), sRépertoire, frzipRépertoire, "")
      					
      				// C'est un fichier
      				cas frzipfichier
      					
      					sFichier  = ExtraitChaîne(sElément, rangDernier, ";")
      					
      					// Vérifie que le fichier se trouve dans la racine en cours
      					SI PAS decompleterep(fExtraitChemin(sFichier, fDisque + fRépertoire)) = sRacine ALORS
      						CONTINUER
      					FIN
      					
      					sInfoFichierComp = zipInfoFichier(sArchive, sFichier)					
      					sInfosZIP = TailleVersChaîne(val(ExtraitChaîne(sInfoFichierComp, 2))) + rc +
      								"Ratio : " + arrondi((Val(ExtraitChaîne(sInfoFichierComp, 3)) * 100 / max(1, Val(ExtraitChaîne(sInfoFichierComp, 2)))), 1) + "%"
      					
      					ZoneRépétéeAjouteLigne(ZR_FICHIERS, IMG_FICHIER, fExtraitChemin(sFichier, fFichier + fExtension), sFichier, frzipFichier, sInfosZIP)
      			FIN
      		FIN			
      		
      
      	fin
